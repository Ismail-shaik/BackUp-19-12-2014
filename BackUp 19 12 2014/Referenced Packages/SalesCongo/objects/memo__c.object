    <validationRules>
        <fullName>preventComplianceAndTest</fullName>
        <active>true</active>
        <description>can not currently have Acceptance compliance AND Test Knowledge checked</description>
        <errorConditionFormula>require_compliance__c &amp;&amp;  test_knowledge__c</errorConditionFormula>
        <errorMessage>You can not require acceptance compliance and also test for knowledge</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>verifyTestSelected</fullName>
        <active>true</active>
        <description>when test required. make sure a test is selected</description>
        <errorConditionFormula>ISBLANK( Test__c ) &amp;&amp;  test_knowledge__c</errorConditionFormula>
        <errorMessage>When Test Knowledge is checked, a test must be selected</errorMessage>
